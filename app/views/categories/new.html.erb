<h1>Category#new</h1>

<%= form_with model: @category, url: categories_path, local: true do |form| %>

  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :icon %>
    <%= form.text_field :icon %>
  </div>

  <div>
    <%= form.submit 'Add Category' %>
  </div>

<% end %>